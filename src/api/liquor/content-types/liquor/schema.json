{
  "kind": "collectionType",
  "collectionName": "liquors",
  "info": {
    "singularName": "liquor",
    "pluralName": "liquors",
    "displayName": "Liquor",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {
    "import-export-entries" : {
      "idField": "title"
    }
  },  "attributes": {
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "videos"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "brand": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::brand.brand",
      "inversedBy": "liquors"
    },
    "category": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::category.category",
      "inversedBy": "liquors"
    },
    "topRated": {
      "type": "boolean",
      "default": false
    },
    "bestSeller": {
      "type": "boolean",
      "default": false
    },
    "staffPicks": {
      "type": "boolean",
      "default": false
    },
    "specials": {
      "type": "boolean",
      "default": false
    },
    "trendingFeatured": {
      "type": "boolean",
      "default": false
    }
  }
}
